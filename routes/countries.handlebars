
<title>Countries</title>

</head>

<body>

{{#each result}}

    <div class="parallax" style="background-image: url('img/{{country}}.jpg')">
        <div class="cityHeader">
            <h1>{{country}}</h1>
        </div> 
    </div>

    <div class="info p-5" style="background-image: url('img/extra_clean_paper.png'">
        <p>{{capital}}</p>
        <p>{{bty}}</p>
        <p>{{foods}}</p>
        <p>{{religions}}</p>
        <p>{{brief_history}}</p>
        <p>{{facts}}</p>
        <p>{{fun_fact}}</p>
    </div>

{{/each}}

</body>