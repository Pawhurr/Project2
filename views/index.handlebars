
<div id="signup">
    <form id="admin-submit">
        <label>signup</label>
        <input type="text" name="signup-username" id="username" required>
        <input type="text" name="signup-email" id="email" required>
        <input type="password" name="signup-password" id="password" required>
        <input type="submit" value="Submit">
    </form>
</div>

<div id="login">
    <form action="/login" method="POST" id="login-submit">
        <label>Login</label>
        <input type="text" name="username" id="login-username" required>
        <input type="password" name="password" id="login-password" required>
        <input type="submit" value="Submit">
    </form>
</div>

</div>



<h1 id="clock" data-name="America/NewYork" data-tz="{{{result.tz}}}"></h1>

<script src="js/clock.js"></script>
<script src="js/admin.js"></script>